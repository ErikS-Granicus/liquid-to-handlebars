{
  "name": "liquid-to-handlebars",
  "description": "Convert liquid templates to handlebars templates.",
  "version": "3.0.0",
  "homepage": "https://github.com/jonschlinkert/liquid-to-handlebars",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/liquid-to-handlebars",
  "bugs": {
    "url": "https://github.com/jonschlinkert/liquid-to-handlebars/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "lth": "bin/cli.js",
    "liquid-to-handlebars": "bin/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-union": "^3.1.0",
    "delete": "^1.1.0",
    "export-files": "^2.1.1",
    "file-is-binary": "^1.0.0",
    "log-ok": "^0.1.1",
    "minimist": "^1.2.0",
    "regex-not": "^1.0.2",
    "snapdragon": "^0.11.3",
    "split-string": "^4.0.0",
    "strip-bom-string": "^1.0.0",
    "through2": "^2.0.3",
    "vinyl-fs": "^3.0.2"
  },
  "devDependencies": {
    "extract-gfm": "^0.1.0",
    "get-value": "^3.0.1",
    "gh-clone": "^1.0.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-format-md": "^1.0.0",
    "handlebars": "^4.0.11",
    "matched": "^2.0.1",
    "mocha": "^3.5.3",
    "write": "^1.0.3"
  },
  "keywords": [
    "assemble",
    "bootstrap",
    "convert",
    "handlebars",
    "jekyll",
    "liquid",
    "refactor",
    "to",
    "transform"
  ],
  "lintDeps": {
    "devDependencies": {
      "files": {
        "patterns": [
          "support/*.js",
          "theme.js"
        ]
      }
    }
  },
  "verb": {
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "handlebars"
      ]
    },
    "reflinks": [
      "handlebars",
      "handlebars-helpers",
      "liquid-filters",
      "template-helpers"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
